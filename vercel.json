{
  "redirects": [
    {
      "source": "http://(.*)",
      "destination": "https://$1",
      "statusCode": 308
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/overview.html"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ]
}
